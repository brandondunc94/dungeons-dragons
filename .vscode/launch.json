    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}//dnd-server//manage.py",
            "args": [
                "runserver"
            ],
            "django": true
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Angular Front End",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/dnd-frontend"
        }
    ]
}